{
  "name": "@uniformdev/optimize-starter-contentful-gatsby",
  "version": "8.0.1",
  "eslintIgnore": [
    "../../packages/**/dist/**/*.js"
  ],
  "description": "Uniform Gatsby starter site for contentful",
  "private": true,
  "scripts": {
    "build": "run-s generate:intents clean build:gatsby",
    "develop": "run-s generate:intents develop:gatsby",
    "build:gatsby": "gatsby build",
    "develop:gatsby": "gatsby develop",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "start": "npm run develop",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "generate:intents": "uniform optimize manifest download --output ./src/lib/intentManifest.json --apiKey $UNIFORM_API_KEY",
    "ci:build": "run-s generate:intents clean build:gatsby"
  },
  "license": "Apache-2.0",
  "dependencies": {
    "@analytics/google-analytics": "^0.5.2",
    "@analytics/segment": "^1.1.0",
    "@n8tb1t/use-scroll-position": "^2.0.3",
    "@uniformdev/optimize-tracker-analytics": "^8.0.1",
    "@uniformdev/optimize-tracker-browser": "^8.0.1",
    "@uniformdev/optimize-tracker-contentful": "^8.0.1",
    "@uniformdev/optimize-tracker-react": "^8.0.1",
    "@uniformdev/optimize-tracker-storage-indexeddb": "^8.0.1",
    "analytics": "^0.7.5",
    "dotenv": "^8.2.0",
    "gatsby": "3.4.0",
    "gatsby-plugin-image": "^1.5.0",
    "gatsby-plugin-postcss": "^4.4.0",
    "gatsby-plugin-react-helmet": "^4.4.0",
    "gatsby-plugin-sharp": "^3.4.0",
    "gatsby-source-contentful": "^5.4.0",
    "js-cookie": "^2.2.1",
    "postcss": "^8.2.13",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-helmet": "^6.1.0"
  },
  "devDependencies": {
    "@uniformdev/cli": "^8.0.1",
    "gatsby-cli": "3.4.0",
    "npm-run-all": "^4.1.5",
    "prettier": "2.2.1",
    "tailwindcss": "^2.1.2"
  }
}
